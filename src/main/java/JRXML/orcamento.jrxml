	<subDataset name="tableDataset" uuid="f13e6d36-5148-4ecc-bbe3-3035def80980">
		<queryString>
			<![CDATA[SELECT 
    o.id_orcamento_,
    c.nome_cliente,
    o.placa,
    o.carro,
    o.data_hora,
    i.descricao,
    i.quantidade,
    i.valor_unitario,
    (i.quantidade * i.valor_unitario) subtotal
FROM 
    orcamentos o
INNER JOIN 
    clientes c ON o.id_cliente = c.id_cliente
INNER JOIN 
    itens_orcamento i ON o.id_orcamento_ = i.id_orcamento_
WHERE 
    o.id_orcamento_ = 1]]>
		</queryString>
	</subDataset>
